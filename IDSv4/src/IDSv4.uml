                <body>for (Nodo nodo1 : gerarchia.getFoglie()) {
         for (Nodo nodo2 : gerarchia.getFoglie()) {
             if (!nodo1.equals(nodo2) &amp;&amp; nodo1.fattoreFoglia(nodo2) == 0) {
                 Double fattore = calcTransitivo(nodo1, nodo2, new ArrayList&lt;>());
