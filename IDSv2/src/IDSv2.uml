              <interfaceRealization xmi:id="_MEUGcBwMEe-urICxG0GjCg" client="_MBA60BwMEe-urICxG0GjCg" supplier="_MD6d0BwMEe-urICxG0GjCg" contract="_MD6d0BwMEe-urICxG0GjCg"/>
