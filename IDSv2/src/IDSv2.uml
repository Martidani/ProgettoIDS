                <ownedParameter xmi:id="_MFBRGRwMEe-urICxG0GjCg" name="radice" type="_MBeN0BwMEe-urICxG0GjCg"/>
                <ownedParameter xmi:id="_MFBRGhwMEe-urICxG0GjCg" type="_MBA60BwMEe-urICxG0GjCg" direction="return">
                  <name xsi:nil="true"/>
                </ownedParameter>
                <language>java</language>
                <body>if (radice.root()!=null) throw new RootTreeException();
     this.foglie = new ArrayList&lt;Nodo>();
     this.radice = radice;
     addFoglie(radice);
 </body>
              </ownedBehavior>
              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_MFBRGxwMEe-urICxG0GjCg" name="addFoglie" specification="_MD7r9RwMEe-urICxG0GjCg">
                <ownedParameter xmi:id="_MFBRHBwMEe-urICxG0GjCg" name="albero" type="_MBeN0BwMEe-urICxG0GjCg"/>
                <language>java</language>
                <body>if (albero.isLeaf()) 
         foglie.add(albero);
     else {
         for (Nodo child : albero.getChildren()) {
             addFoglie(child);
         }
     }
 </body>
              </ownedBehavior>
              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_MFBRHRwMEe-urICxG0GjCg" name="toString" specification="_MBBh4BwMEe-urICxG0GjCg">
