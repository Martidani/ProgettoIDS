		Nodo radice = gerarchia.visualizzaRadice(nomeRadice);
		System.out.println(radice.toNavigationString() + &quot;\n&quot;);
		Nodo child;
				
		do {
			int valoreDominio = InputDati.leggiIntero(&quot;Scegli l'opzione -> &quot;);	
