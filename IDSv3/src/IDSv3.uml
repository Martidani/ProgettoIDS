              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_XYFEQhwNEe-urICxG0GjCg" name="calcTransitivo" specification="_XXmjJBwNEe-urICxG0GjCg">
                <ownedParameter xmi:id="_XYFEQxwNEe-urICxG0GjCg" name="nodo1" type="_XU8QsBwNEe-urICxG0GjCg"/>
                <ownedParameter xmi:id="_XYFERBwNEe-urICxG0GjCg" name="nodo2" type="_XU8QsBwNEe-urICxG0GjCg"/>
                <ownedParameter xmi:id="_XYFERRwNEe-urICxG0GjCg" name="visitati" type="_XXRzABwNEe-urICxG0GjCg"/>
