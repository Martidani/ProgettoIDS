              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_XYDPKBwNEe-urICxG0GjCg" name="visualizza" specification="_XXkt8BwNEe-urICxG0GjCg">
                <ownedParameter xmi:id="_XYDPKRwNEe-urICxG0GjCg" name="nomeNodo">
                  <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
                </ownedParameter>
                <ownedParameter xmi:id="_XYDPKhwNEe-urICxG0GjCg" name="root">
